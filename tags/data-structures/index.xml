&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> &lt;span class="n">today&lt;/span> &lt;span class="o">=&lt;/span> &lt;span class="nf">Sys.Date&lt;/span>&lt;span class="p">())&lt;/span>
